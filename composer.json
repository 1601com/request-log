{
  "name": "agentur1601com/request-log",
  "type": "contao-bundle",
  "description": "Logs data for page views and automatically deletes them after configured time.",
  "homepage": "https://www.1601.com/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Florian Scheyer"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "contao/core-bundle": "^4.4",
    "symfony/config": "^4.4 || ^5.1",
    "symfony/dependency-injection": "^4.4 || ^5.1",
    "symfony/http-kernel": "^4.4 || ^5.1"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "extra": {
    "contao-manager-plugin": "Agentur1601com\\RequestLog\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Agentur1601com\\RequestLog\\": "src/"
    }
  }
}
